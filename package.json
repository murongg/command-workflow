{
  "name": "command-workflow",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.6.0",
  "description": "",
  "author": "MuRong <hi@mrong.me>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/murongg",
  "homepage": "https://github.com/murongg/command-workflow#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murongg/command-workflow.git"
  },
  "bugs": {
    "url": "https://github.com/murongg/command-workflow/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "cwf": "bin/run.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "local-pkg": "^0.4.3",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@antfu/utils": "^0.7.4",
    "@murongg/eslint-config": "^0.2.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^18.16.16",
    "@vitest/coverage-istanbul": "^0.33.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "simple-git-hooks": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.31.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
